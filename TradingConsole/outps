//one for all portfolios
$(function() {
	var $cardWrapper = $('.cardWrapper');
    $( "#dialog-confirm-delete-position" ).dialog({
           autoOpen: false,
           width: 475,
           dialogClass: "generalPopup",
           resizable: false,
           draggable: false,
           modal: true,
           closeText: "Close",
           title:'Delete Position',
           buttons: {
                  "Delete": function() {

                      var $this = $("#dialog-confirm-delete-position");
                      var portfolio_id = $this.data('portfolioid');
                      var position_id = $this.data('positionid');
                      var position_type=$this.data('positiontype');
                      var $el = $this.data('position_el'),
						  numOfLines = $this.data('lineCount');

                      function slideUPRow(){
						  $el.slideUp(250, function(){
							  $el.remove();
						  })
                      }

                      if(position_type == 'open'){
						  window.removeOpenPosition($el, portfolio_id, position_id, numOfLines);
                       }

                       if(position_type == 'close')
                       {
                           slideUPRow();
                           removeClosePosition(portfolio_id, position_id);
                       }

                      Portfolio.updateInstrumentCount(portfolio_id, -1);
               	 
                   $(this)
                       .data('portfolioid','')
                       .data('positionid', '')
                       .data('positiontype','')
                       .dialog( "close" );
               },

			    "Cancel": function() {
			        $( this ).dialog( "close" );
			        return false;
			    }
               
           },
           open: function() {
               $('.ui-dialog-buttonpane').find('button').addClass('newBtn toggleButton noIcon LightGray');
           }
    });
});


    $(document).ready(function() {

                var portfolioBlock = $("#portfolioblock_NDA1ZmUzYTVkMmtvZDU1Mw==");
        if (portfolioBlock.length) {
            $('html, body').animate({
                scrollTop: portfolioBlock.offset().top - 50 - 48
            }, 600);
        }
        
        components.headers.setHeaderSwitchBorders('#fullColumn', '#footerWrapper');
        components.headers.onChange(function(isFloatedHeaderShown) {
            $('#addPortfolioPopup').addClass('displayNone');
            $('#portfolios_choose_list').addClass('displayNone');
        });
    })
</script>

